<% if (title) { %>
    <h1><%= title %></h1>
<% } else { %>
    <%- res.redirect('/') %>
    <% return; %> <!-- Adiciona return para interromper a execução -->
<% } %>
<%- messages() %> 

<h3>Welcome <%- user.username %></h3>
<p>You're logged in.</p>
<a href="/account/update/<%- user.id %>">Edit Account Information</a>
<% if ( user.role != 'Client') { %>
    <h3>Inventory Management</h3>
    <a href="/inv/">Manage Inventory</a>
<% } %>
